        "from time import time\n",
        "start = time()\n",
        "\n",
        "# Python program to create acronyms\n",
        "word = \"Akshaya LN\"\n",
        "text = word.split()\n",
        "a = \" \"\n",
        "for i in text:\n",
        "    a = a+str(i[0]).upper()\n",
        "print(a)\n",
        "\n",
        "end = time()\n",
        "execution_time = end - start\n",
        "print(\"Execution Time : \", execution_time)"
      ]
