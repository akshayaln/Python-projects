      "source": [
        "from spellchecker import SpellChecker\n",
        "corrector = SpellChecker()\n",
        "\n",
        "word = input(\"Enter a Word : \")\n",
        "if word in corrector:\n",
        "    print(\"Correct\")\n",
        "else:\n",
        "    correct_word = corrector.correction(word)\n",
        "    print(\"Correct Spelling is \", correct_word)"
      ]
    },
